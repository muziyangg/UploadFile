{
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "devCommand": "echo 'Starting development server'",
  "framework": null,
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/*.js": {
      "includeFiles": "**/*",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "env": {
    "NODE_VERCEL": "1"
  }
}
